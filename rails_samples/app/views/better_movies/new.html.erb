<h1>New movie</h1>

<%# codecite routing %>
<% remote_form_for(@movie, :url => {:action => :create}) do |f| %>
  <p>
    <label>Name:</label>
    <%= f.text_field :title %>
  </p>
  <p>
    <label>Year:</label>
    <%= f.text_field :year %>
  </p>

  <p>
    <%= f.submit "Add Movie" %>
  </p>
<% end %>

